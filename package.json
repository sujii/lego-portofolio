{
  "name": "@sujii/lego-portofolio",
  "version": "0.9.1",
  "dependencies": {
    "@repo/lego-portofolio-api-client": "workspace:*",
    "@repo/lego-portofolio-typescript-config": "workspace:*",
    "@repo/lego-portofolio-ui": "workspace:*",
    "@repo/lego-portofolio-web": "workpace:*"
  },
  "devDependencies": {
    "prettier": "^3.4.1",
    "turbo": "^2.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "lisence": "GPL-3.0 license",
  "packageManager": "yarn@4.5.3",
  "private": true,
  "scripts": {
    "api-client": "yarn workspace @repo/lego-portofolio-api-client",
    "build": "turbo build",
    "dev": "turbo dev",
    "format": "turbo lint -- --fix",
    "lint": "turbo lint",
    "prepare": "husky install",
    "pritty": "prettier --write \"**/*.{ts,tsx,md}\"",
    "web": "yarn workspace @repo/lego-portofolio-web"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
